{
	"name": "@fslzrr/create-new-project",
	"version": "1.0.0",
	"description": "CLI tool that scaffolds new projects",
	"bin": "bin/index.js",
	"files": ["bin", "dist"],
	"scripts": {
		"clean": "rm -rf dist",
		"predev": "pnpm clean",
		"dev": "tsc --watch",
		"prebuild": "pnpm clean",
		"build": "tsc",
		"check": "biome check && tsc --noEmit",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"test": "jest",
		"prepare": "husky",
		"precommit": "biome check --staged --no-errors-on-unmatched"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fslzrr/create-new-project.git"
	},
	"keywords": ["CLI", "'New", "project'", "Node", "Typescript"],
	"author": "Fernando Salazar <fslzrr@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/fslzrr/create-new-project/issues"
	},
	"homepage": "https://github.com/fslzrr/create-new-project#readme",
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/jest": "^29.5.12",
		"husky": "^9.1.4",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4"
	},
	"packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
